        <h2
        class="text-2xl font-semibold text-gray-700 lg:text-3xl dark:text-white scroll-mt-20"
        >
        Valores
        </h2>
    </div>
    <CardOnePadre>
        {
        valores.map((item) => (
            <CardOneItem 
            title={item.title}
            content={item.content}
            bg={item.bg}
            svg={item.svg}
            />
        ))
        }          
    </CardOnePadre>
    </div>
</Section>